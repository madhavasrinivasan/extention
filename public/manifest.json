{
    "name": "AI Summarizer",
    "description": "summarizes the content of your webpage",
    "version": "1.0.0",
    "manifest_version": 3,
    "author": "Madhav Srinivasn",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": []
  }